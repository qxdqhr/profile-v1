{
  "name": "sa2kit",
  "version": "1.0.0",
  "description": "Super Anime 2D/3D Kit - A comprehensive MMD model viewer and animation player library based on Three.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mmd",
    "mikumikudance",
    "three.js",
    "3d",
    "animation",
    "pmx",
    "pmd",
    "vmd",
    "react",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sa2kit.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/sa2kit/issues"
  },
  "homepage": "https://github.com/yourusername/sa2kit#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "three": "^0.160.0"
  },
  "dependencies": {
    "three-stdlib": "^2.29.0",
    "mmd-parser": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "sideEffects": false
}

